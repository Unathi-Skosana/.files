! Font Settings
#include ".x/fonts"

! URxvt
#include ".x/urxvt"

! X colors.
! Generated by 'wal'
*foreground:        #dbf0ef
*background:        #2d2f39
*.foreground:       #dbf0ef
*.background:       #2d2f39
emacs*foreground:   #dbf0ef
emacs*background:   #2d2f39
URxvt*foreground:   #dbf0ef
XTerm*foreground:   #dbf0ef
UXTerm*foreground:  #dbf0ef
URxvt*background:   [100]#2d2f39
XTerm*background:   #2d2f39
UXTerm*background:  #2d2f39
URxvt*cursorColor:  #dbf0ef
XTerm*cursorColor:  #dbf0ef
UXTerm*cursorColor: #dbf0ef
URxvt*borderColor:  [100]#2d2f39

! Colors 0-15.
*.color0: #2d2f39
*color0:  #2d2f39
*.color1: #91BEBB
*color1:  #91BEBB
*.color2: #D38CB3
*color2:  #D38CB3
*.color3: #FDB1BD
*color3:  #FDB1BD
*.color4: #C9BBC1
*color4:  #C9BBC1
*.color5: #A8DDDC
*color5:  #A8DDDC
*.color6: #EAD6D5
*color6:  #EAD6D5
*.color7: #dbf0ef
*color7:  #dbf0ef
*.color8: #99a8a7
*color8:  #99a8a7
*.color9: #91BEBB
*color9:  #91BEBB
*.color10: #D38CB3
*color10:  #D38CB3
*.color11: #FDB1BD
*color11:  #FDB1BD
*.color12: #C9BBC1
*color12:  #C9BBC1
*.color13: #A8DDDC
*color13:  #A8DDDC
*.color14: #EAD6D5
*color14:  #EAD6D5
*.color15: #dbf0ef
*color15:  #dbf0ef

! Black color that will not be affected by bold highlighting.
*.color66: #2d2f39
*color66:  #2d2f39

! Xclock colors.
XClock*foreground: #dbf0ef
XClock*background: #2d2f39
XClock*majorColor:  rgba:db/f0/ef/ff
XClock*minorColor:  rgba:db/f0/ef/ff
XClock*hourColor:   rgba:db/f0/ef/ff
XClock*minuteColor: rgba:db/f0/ef/ff
XClock*secondColor: rgba:db/f0/ef/ff

! Set depth to make transparency work.
URxvt*depth: 32
